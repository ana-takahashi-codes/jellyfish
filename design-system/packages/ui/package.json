{
  "name": "@jellyfish/ui",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "./fonts.css": "./src/fonts.css",
    "./variants": {
      "types": "./src/variants.ts",
      "import": "./dist/variants.mjs",
      "require": "./dist/variants.js"
    },
    "./tokens": {
      "types": "./src/tokens.ts",
      "import": "./dist/tokens.mjs",
      "require": "./dist/tokens.js"
    },
    "./icon": {
      "types": "./src/components/icon/index.ts",
      "import": "./dist/components/icon/index.mjs",
      "require": "./dist/components/icon/index.js"
    },
    "./theme": {
      "types": "./src/theme/index.ts",
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js"
    },
    "./control": {
      "types": "./src/components/utils/control/index.ts",
      "import": "./dist/control.mjs",
      "require": "./dist/control.js"
    },
    "./button": {
      "types": "./src/components/button/index.ts",
      "import": "./dist/button.mjs",
      "require": "./dist/button.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@jellyfish/tokens": "workspace:*",
    "@tabler/icons-react": "^3.36.1",
    "react": "^18.2.0",
    "styled-components": "^6.x"
  },
  "publishConfig": {
    "access": "public"
  }
}