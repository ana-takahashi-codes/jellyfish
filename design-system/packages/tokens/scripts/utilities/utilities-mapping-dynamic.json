        },
        "page": {
          "default": "jf-color-bg-page"
        },
        "neutral": {
          "default": "jf-color-bg-neutral"
        },
        "neutral-soft": {
          "default": "jf-color-bg-neutral-soft"
        },
        "accent": {
          "default": "jf-color-bg-accent"
        },
        "accent-soft": {
          "default": "jf-color-bg-accent-soft"
        },
        "brand-primary": {
          "default": "jf-color-bg-brand-primary"
        },
        "brand-primary-soft": {
          "default": "jf-color-bg-brand-primary-soft"
        },
        "warning": {
          "default": "jf-color-bg-warning"
        },
        "warning-soft": {
          "default": "jf-color-bg-warning-soft"
        },
        "critical": {
          "default": "jf-color-bg-critical"
        },
        "critical-soft": {
          "default": "jf-color-bg-critical-soft"
        },
        "positive": {
