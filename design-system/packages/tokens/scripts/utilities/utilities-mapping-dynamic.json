  "borderColor": {
    "prefix": "bd",
    "properties": "border-color",
    "tokenPattern": "jf-color-border-",
    "classPattern": "{prefix}-{color}",
    "dynamicMapping": {
      "color": {
        "muted": {
          "default": "jf-color-border-muted"
        },
        "moderated": {
          "default": "jf-color-border-moderated"
