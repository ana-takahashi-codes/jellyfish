{
  "spacing": {
    "prefix": ["p", "m"],
    "responsive": true,
    "properties": ["padding", "margin"],
    "tokenPattern": "jf-size-",
    "classPattern": "{prefix}{props}-{variant}",
    "dynamicMapping": {
      "props": {
        "":[["padding"],["margin"]],
        "t": [["padding-top"], ["margin-top"]],
        "r": [["padding-right"], ["margin-right"]],
        "b": [["padding-bottom"], ["margin-bottom"]],
        "l": [["padding-left"], ["margin-left"]],
        "x": [
          ["padding-left", "padding-right"],
          ["margin-left", "margin-right"]
        ],
        "y": [
          ["padding-top", "padding-bottom"],
          ["margin-top", "margin-bottom"]
        ]
      },
      "variant": {
        "1": "jf-size-1",
        "2": "jf-size-2",
        "3": "jf-size-3",
        "4": "jf-size-4",
        "5": "jf-size-5",
        "6": "jf-size-6",
        "7": "jf-size-7",
        "8": "jf-size-8",
        "10": "jf-size-10",
        "12": "jf-size-12",
        "14": "jf-size-14",
        "16": "jf-size-16",
        "18": "jf-size-18",
        "20": "jf-size-20",
        "22": "jf-size-22",
        "24": "jf-size-24",
        "28": "jf-size-28",
        "32": "jf-size-32",
        "36": "jf-size-36",
        "40": "jf-size-40",
        "56": "jf-size-56",
        "72": "jf-size-72",
        "100": "jf-size-100",
        "180": "jf-size-180",
        "260": "jf-size-260",
        "320": "jf-size-320",
        "400": "jf-size-400",
        "full": "jf-size-full",
        "half": "jf-size-half",
        "third": "jf-size-third",
        "quarter": "jf-size-quarter"
      }
    }
  },
  "background": {
    "prefix": "bg",
    "properties": "background-color",
    "tokenPattern": "jf-color-bg-",
