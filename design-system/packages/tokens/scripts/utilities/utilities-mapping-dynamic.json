{
  "spacing": {
    "prefix": ["p", "m"],
    "responsive": true,
    "properties": ["padding", "margin"],
    "tokenPattern": "jf-size-",
    "classPattern": "{prefix}-{props}-{variant}",
    "patternDimensionName": "variant",
    "generateFromPattern": true,
    "dynamicMapping": {
      "props": {
        "":[["padding"],["margin"]],
        "t": [["padding-top"], ["margin-top"]],
        "r": [["padding-right"], ["margin-right"]],
        "b": [["padding-bottom"], ["margin-bottom"]],
        "l": [["padding-left"], ["margin-left"]],
        "x": [
          ["padding-left", "padding-right"],
          ["margin-left", "margin-right"]
        ],
        "y": [
          ["padding-top", "padding-bottom"],
          ["margin-top", "margin-bottom"]
        ]
      }
    }
  },
  "background": {
    "prefix": "bg",
    "properties": "background-color",
    "tokenPattern": "jf-color-bg-",
    "classPattern": "{prefix}-{color}",
    "patternDimensionName": "color",
    "generateFromPattern": true
  },

  "textColor": {
    "prefix": "fg",
    "properties": "color",
    "tokenPattern": "jf-color-fg-",
    "classPattern": "{prefix}-{color}",
    "patternDimensionName": "color",
    "generateFromPattern": true
  },
  "gap": {
    "prefix": "gap",
    "responsive": true,
    "properties": "gap",
    "tokenPattern": "jf-size-",
    "classPattern": "{prefix}-{props}-{variant}",
    "patternDimensionName": "variant",
    "generateFromPattern": true,
    "dynamicMapping": {
      "props": {
        "": [["gap"]],
        "x": [["column-gap"]],
        "y": [["row-gap"]]
      }
    }
  },
  "sizing": {
    "prefix": ["w", "h"],
    "responsive": true,
    "properties": ["width", "height", "min-width", "min-height", "max-width", "max-height"],
    "tokenPattern": "jf-size-",
    "classPattern": "{prefix}-{props}-{size}",
    "patternDimensionName": "size",
    "generateFromPattern": true,
    "dynamicMapping": {
      "props": {
        "": [["width"], ["height"]],
        "min": [["min-width"], ["min-height"]],
        "max": [["max-width"], ["max-height"]]
      }
    }
  },
  "size": {
    "prefix": "size",
    "responsive": true,
    "properties": ["width", "height"],
    "tokenPattern": "jf-size-",
    "classPattern": "{prefix}-{variant}",
    "patternDimensionName": "variant",
    "generateFromPattern": true
  },
  "borderColor": {
    "prefix": "bd",
    "properties": "border-color",
    "tokenPattern": "jf-color-bd-",
    "classPattern": "{prefix}-{color}",
    "patternDimensionName": "color",
    "generateFromPattern": true
  },
  "border": {
    "prefix": ["bd"],
    "responsive": true,
    "properties": ["border-width"],
    "tokenPattern": "jf-bd-width-",
    "classPattern": "{prefix}-{props}-{width}",
    "dynamicMapping": {
      "props": {
        "": [["border-width"]],
        "t": [["border-top-width"]],
        "r": [["border-right-width"]],
        "b": [["border-bottom-width"]],
        "l": [["border-left-width"]],
        "x": [["border-left-width", "border-right-width"]],
        "y": [["border-top-width", "border-bottom-width"]]
      },
      "width": {
        "0-5": "jf-bd-width-0-5",
        "1": "jf-bd-width-1",
        "2": "jf-bd-width-2",
        "4": "jf-bd-width-4"
      }
    }
  },
  "borderRadius": {
    "prefix": "corner",
    "properties": "border-radius",
    "tokenPattern": "jf-corner-radius-",
    "classPattern": "{prefix}-{size}",
    "dynamicMapping": {
      "size": {
        "0": "jf-corner-radius-0",
        "2": "jf-corner-radius-2",
        "3": "jf-corner-radius-3",
        "4": "jf-corner-radius-4",
        "6": "jf-corner-radius-6",
        "12": "jf-corner-radius-12",
        "20": "jf-corner-radius-20",
        "40": "jf-corner-radius-40",
        "sm": "jf-corner-radius-sm",
        "default": "jf-corner-radius-default",
        "lg": "jf-corner-radius-lg",
        "xl": "jf-corner-radius-xl",
        "circle": "jf-corner-radius-circle",
        "pill": "jf-corner-radius-pill"
      }
    }
  },
  "angle": {
    "prefix": "angle",
    "properties": "rotate",
    "tokenPattern": "jf-angle-",
    "classPattern": "{prefix}-{variant}",
    "dynamicMapping": {
      "variant": {
        "1": "jf-angle-1",
        "2": "jf-angle-2",
        "3": "jf-angle-3",
        "4": "jf-angle-4",
        "5": "jf-angle-5",
        "6": "jf-angle-6",
        "7": "jf-angle-7",
        "8": "jf-angle-8",
        "9": "jf-angle-9",
        "10": "jf-angle-10"
      }
    }
  }
}
