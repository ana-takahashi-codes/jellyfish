    "responsive": true,
    "properties": ["border-width"],
    "tokenPattern": "jf-border-width-",
    "classPattern": "{prefix}{props}-{width}",
    "dynamicMapping": {
      "props": {
        "": [["border-width"]],
        "t": [["border-top-width"]],
