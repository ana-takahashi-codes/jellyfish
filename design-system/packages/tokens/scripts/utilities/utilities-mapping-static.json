  "sizing": {
    "prefix": ["w", "h"],
    "responsive": true,
    "properties": ["width", "height", "min-width", "min-height", "max-width", "max-height"],
    "tokenPattern": "jf-size-",
    "classPattern": "{prefix}-{props}-{size}",
    "dynamicMapping": {
      "props": {
        "": [["width"], ["height"]],
        "min": [["min-width"], ["min-height"]],
        "max": [["max-width"], ["max-height"]]
      },
      "size": {
      "auto": "auto",
      "half": "var(--jf-size-half)",
      "third": "var(--jf-size-third)",
      "one-third": "var(--jf-size-third)",
      "two-thirds": "66.66%",
      "quarter": "var(--jf-size-quarter)",
      "20pct": "20%",
      "40pct": "40%",
      "60pct": "60%",
      "75pct": "75%",
      "80pct": "80%",
      "100pct": "100%",
      "max-content": "max-content",
      "min-content": "min-content",
      "fit": "fit-content"
    }
  }},
