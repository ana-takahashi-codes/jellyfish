{
  "name": "@jellyfish/tokens",
  "version": "0.0.0",
  "description": "Jellyfish Design System Tokens",
  "keywords": [
    "design system",
    "design tokens",
    "jellyfish",
    "Ana Takahashi"
  ],
  "author": "Ana Takahashi",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./build/css/themes/core/primitives.css",
  "style": "./build/css/themes/core/primitives.css",
  "files": [
    "build",
    "config",
    "scripts",
    "src/vars.js",
    "src/vars.d.ts",
    "src/css",
    "src/fonts",
    "src/tokens-studio/*.json",
    "tokens.resolver.json"
  ],
  "exports": {
    ".": {
      "style": "./build/css/themes/core/primitives.css",
      "default": "./build/css/themes/core/primitives.css"
    },
    "./config": {
      "import": "./config/index.js",
      "default": "./config/index.js"
    },
    "./build/*": "./build/*",
    "./jit": "./scripts/utilities-jit/postcss-plugin.mjs",
    "./vars": {
      "types": "./src/vars.d.ts",
      "import": "./src/vars.js",
      "default": "./src/vars.js"
    },
    "./interactive-states.css": "./src/css/interactive-states.css",
    "./fonts.css": "./src/css/fonts.css"
  },
  "scripts": {
    "build": "node scripts/build/build.js && node scripts/utilities/utils-build.mjs",
    "tokens:split": "node scripts/build/split-sets.mjs",
    "tokens:clean": "node scripts/build/clean.js",
    "tokens:utilities": "node scripts/utilities/utils-build.mjs",
    "tokens:jit-example": "node scripts/utilities-jit/run-jit-example.mjs",
    "version:patch": "pnpm version patch",
    "version:minor": "pnpm version minor",
    "version:major": "pnpm version major",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@tokens-studio/sd-transforms": "^2.0.3",
    "postcss": "^8.4.49",
    "style-dictionary": "^5.1.4"
  }
}