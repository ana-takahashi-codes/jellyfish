{
  "name": "@jellyfish/tokens",
  "version": "0.0.0",
  "description": "Jellyfish Design System Tokens",
  "keywords": ["design system", "design tokens", "jellyfish", "Ana Takahashi"],
  "author": "Ana Takahashi",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./build/css/themes/core/primitives.css",
  "style": "./build/css/themes/core/primitives.css",
  "files": ["build", "config", "scripts", "src"],
  "exports": {
    ".": {
      "style": "./build/css/themes/core/primitives.css",
      "default": "./build/css/themes/core/primitives.css"
    },
    "./config": {
      "import": "./config/index.js",
      "default": "./config/index.js"
    },
    "./build/*": "./build/*"
  },
  "scripts": {
    "dev": "echo 'Add dev script here'",
    "build": "node scripts/build/build.js",
    "tokens:build": "node scripts/build/build.js",
    "tokens:clean": "node scripts/build/clean.js",
    "tokens:utilities": "node scripts/utilities/utils-build.mjs",
    "tokens:validate-js-dts": "node scripts/validate/validate-js-dts-consistency.js",
    "tokens:scan-deprecated": "node scripts/deprecated/scan-deprecated-usage.js",
    "tokens:deprecated-report": "node scripts/deprecated/deprecated-report.js",
    "version:patch": "pnpm version patch",
    "version:minor": "pnpm version minor",
    "version:major": "pnpm version major",
    "prepublishOnly": "pnpm run build",
    "test": "echo 'Add test script here'",
    "lint": "echo 'Add lint script here'"
  },
  "devDependencies": {
    "@tokens-studio/sd-transforms": "^2.0.3",
    "style-dictionary": "^5.1.4"
  }
}
